<regression_suite name="nightly_ocean_test_suite">
	<test name="Drying slope 10km -- RK4" core="ocean" configuration="drying_slope" resolution="10km" test="default_RK4">
		<script name="run_test.py"/>
	</test>
	<test name="Overflow 10km" core="ocean" configuration="overflow" resolution="10km" test="default">
		<script name="run_test.py"/>
	</test>
	<test name="Overflow 10km -- drying prevention on" core="ocean" configuration="overflow" resolution="10km" test="default_prevent_drying">
		<script name="run_test.py"/>
	</test>
	<test name="Drying due to wave on slope 10km - RK4 " core="ocean" configuration="onshore_wave" resolution="10km" test="drying_RK4">
		<script name="run_test.py"/>
	</test>
	<test name="No drying due to wave on slope 10km - RK4" core="ocean" configuration="onshore_wave" resolution="10km" test="default_RK4">
		<script name="run_test.py"/>
	</test>
	<test name="No drying due to wave on slope 10km - split explicit" core="ocean" configuration="onshore_wave" resolution="10km" test="default">
		<script name="run_test.py"/>
	</test>
	<test name="Drying due to wave on slope 10km - split explicit" core="ocean" configuration="onshore_wave" resolution="10km" test="drying">
		<script name="run_test.py"/>
	</test>
</regression_suite>
